{
    "name": "sef/translatable",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "translation",
        "language",
        "database"
    ],
    "description": "The Hyperf Multilingual Model package.",
    "autoload": {
        "psr-4": {
            "Easy\\Translatable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Easy\\Translatable\\": "tests"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-swoole": ">=5.0",
        "hyperf/config": "~3.1.0",
        "hyperf/database": "~3.1.0",
        "hyperf/db-connection": "~3.1.0",
        "hyperf/di": "~3.1.0",
        "hyperf/framework": "~3.1.0",
        "hyperf/model-listener": "~3.1.0",
        "hyperf/translation": "~3.1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "hyperf/testing": "3.0.*",
        "phpstan/phpstan": "^1.0",
        "swoole/ide-helper": "^5.0",
        "mockery/mockery": "^1.0"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "co-phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Easy\\Translatable\\ConfigProvider"
        }
    }
}
